msgid "Server.WaitingForTv"
msgstr "En attente de connexion du TV..."

msgid "Server.InstallDetected"
msgstr "Installation détectée !"

#: Completed Page
msgid "Completed.Title"
msgstr "Installation terminée !"

msgid "Completed.Message"
msgstr "Jellyfin a été installé avec succès sur votre TV."

msgid "Completed.WhatsNext"
msgstr "Et maintenant ?"

msgid "Completed.Step1"
msgstr "Ouvrez le Smart Hub sur votre TV"

msgid "Completed.Step2"
msgstr "Trouvez et lancez l'application Jellyfin"

msgid "Completed.Step3"
msgstr "Configurez votre serveur Jellyfin"

