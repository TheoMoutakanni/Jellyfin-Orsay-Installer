#: Network Setup Page
msgid "Network.Title"
msgstr "Cài đặt mạng"

msgid "Network.SelectIp"
msgstr "Chọn địa chỉ IP của máy tính:"

msgid "Network.Port"
msgstr "Cổng máy chủ:"

msgid "Network.PortHint"
msgstr "Mặc định là 80. Chỉ thay đổi khi cần thiết."

msgid "Network.SameNetworkHint"
msgstr "TV phải cùng mạng với giao diện được chọn."

msgid "Network.GroupOther"
msgstr "— Khác —"

msgid "Network.GroupVirtual"
msgstr "— Ảo —"

#: TV Setup Page
msgid "TvSetup.Title"
msgstr "Cài đặt TV"

msgid "TvSetup.IpBanner"
msgstr "Nhập địa chỉ IP này vào TV:"

msgid "TvSetup.SelectSeries"
msgstr "Chọn dòng TV:"

msgid "TvSetup.SeriesE"
msgstr "Dòng E (2012)"

msgid "TvSetup.SeriesF"
msgstr "Dòng F (2013)"

msgid "TvSetup.SeriesH"
msgstr "Dòng H (2014-15)"

msgid "TvSetup.DontKnowSeries"
msgstr "Không chắc? Kiểm tra số model ở mặt sau TV (ví dụ: UE40ES6100)"

msgid "TvSetup.ScanForTvs"
msgstr "Quét mạng tìm TV"

msgid "TvSetup.DetectedTv"
msgstr "Đã phát hiện:"

msgid "TvSetup.NextHint"
msgstr "Sau khi đọc hướng dẫn, nhấp 'Tiếp theo' để khởi động máy chủ. Sau đó làm theo các bước trên TV."

#: TV Setup - Series E (2012)
msgid "TvSetup.E.Step1.Title"
msgstr "Mở Smart Hub"

msgid "TvSetup.E.Step1.Desc"
msgstr "Nhấn nút Smart Hub trên điều khiển, sau đó nhấn nút A đỏ để mở menu đăng nhập."

msgid "TvSetup.E.Step2.Title"
msgstr "Đăng nhập với tư cách Nhà phát triển"

msgid "TvSetup.E.Step2.Desc"
msgstr "Nhập 'develop' làm tên tài khoản. TV sẽ tạo mật khẩu - ghi lại. Nhấn Đăng nhập."

msgid "TvSetup.E.Step3.Title"
msgstr "Mở Cài đặt Nhà phát triển"

msgid "TvSetup.E.Step3.Desc"
msgstr "Nhấn nút Tools trên điều khiển, vào Cài đặt, sau đó chọn Nhà phát triển."

msgid "TvSetup.E.Step4.Title"
msgstr "Nhập địa chỉ IP"

msgid "TvSetup.E.Step4.Desc"
msgstr "Chọn 'Địa chỉ IP' và nhập: {0}"

msgid "TvSetup.E.Step5.Title"
msgstr "Đồng bộ ứng dụng"

msgid "TvSetup.E.Step5.Desc"
msgstr "Chọn 'Đồng bộ ứng dụng người dùng' để bắt đầu cài đặt. Ứng dụng sẽ xuất hiện trong Smart Hub."

#: TV Setup - Series F (2013)
msgid "TvSetup.F.Step1.Title"
msgstr "Mở Tài khoản Samsung"

msgid "TvSetup.F.Step1.Desc"
msgstr "Nhấn nút Menu, vào Tính năng thông minh, sau đó chọn Tài khoản Samsung."

msgid "TvSetup.F.Step2.Title"
msgstr "Đăng nhập với tư cách Nhà phát triển"

msgid "TvSetup.F.Step2.Desc"
msgstr "Nhập 'develop' làm email. Để trống mật khẩu (hoặc thử 'sso1029dev!' nếu cần). Nhấn Đăng nhập."

msgid "TvSetup.F.Step3.Title"
msgstr "Mở Cài đặt ứng dụng"

msgid "TvSetup.F.Step3.Desc"
msgstr "Nhấn Smart Hub, vào Thêm ứng dụng (góc dưới phải), sau đó nhấn Tùy chọn."

msgid "TvSetup.F.Step4.Title"
msgstr "Nhập địa chỉ IP"

msgid "TvSetup.F.Step4.Desc"
msgstr "Chọn 'Cài đặt IP' và nhập: {0}"

msgid "TvSetup.F.Step5.Title"
msgstr "Đồng bộ ứng dụng"

msgid "TvSetup.F.Step5.Desc"
msgstr "Chọn 'Bắt đầu đồng bộ ứng dụng' để bắt đầu cài đặt."

msgid "TvSetup.F.Step6.Title"
msgstr "Khởi động lại nếu cần"

msgid "TvSetup.F.Step6.Desc"
msgstr "Nếu ứng dụng không xuất hiện, hãy khởi động lại TV và kiểm tra lại Smart Hub."

#: TV Setup - Series H (2014-15)
msgid "TvSetup.H.Step1.Title"
msgstr "Mở Tài khoản Samsung"

msgid "TvSetup.H.Step1.Desc"
msgstr "Vào Smart Hub, chọn Tài khoản Samsung, sau đó chọn Đăng nhập."

msgid "TvSetup.H.Step2.Title"
msgstr "Đăng nhập với tư cách Nhà phát triển"

msgid "TvSetup.H.Step2.Desc"
msgstr "Nhập 'develop' làm tên đăng nhập. Bỏ qua trường mật khẩu. Đánh dấu 'Ghi nhớ' và nhấn Đăng nhập."

msgid "TvSetup.H.Step3.Title"
msgstr "Mở Menu đồng bộ"

msgid "TvSetup.H.Step3.Desc"
msgstr "Trong Smart Hub, hướng điều khiển vào ứng dụng bất kỳ và giữ tâm D-pad cho đến khi menu xuất hiện."

msgid "TvSetup.H.Step4.Title"
msgstr "Nhập địa chỉ IP"

msgid "TvSetup.H.Step4.Desc"
msgstr "Chọn 'Cài đặt IP' và nhập: {0} (xác nhận từng nhóm số bằng tâm D-pad)."

msgid "TvSetup.H.Step5.Title"
msgstr "Bắt đầu đồng bộ"

msgid "TvSetup.H.Step5.Desc"
msgstr "Mở lại menu và chọn 'Start User App Sync'."

msgid "TvSetup.H.Step6.Title"
msgstr "Hoàn tất cài đặt"

msgid "TvSetup.H.Step6.Desc"
msgstr "Chấp nhận thông báo cài đặt. Thoát Smart Hub và vào lại để xem các ứng dụng đã cài."

