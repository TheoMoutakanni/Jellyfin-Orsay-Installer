msgstr "— Virtuale —"

#: TV Setup Page
msgid "TvSetup.Title"
msgstr "Configurare TV"

msgid "TvSetup.IpBanner"
msgstr "Introdu această adresă IP pe TV:"

msgid "TvSetup.SelectSeries"
msgstr "Selectează seria TV-ului:"

msgid "TvSetup.SeriesE"
msgstr "Seria E (2012)"

msgid "TvSetup.SeriesF"
msgstr "Seria F (2013)"

msgid "TvSetup.SeriesH"
msgstr "Seria H (2014-15)"

msgid "TvSetup.DontKnowSeries"
msgstr "Nu ești sigur? Verifică numărul modelului pe spatele TV-ului (ex. UE40ES6100)"

msgid "TvSetup.ScanForTvs"
msgstr "Scanează rețeaua pentru TV-uri"

msgid "TvSetup.DetectedTv"
msgstr "Detectat:"

msgid "TvSetup.NextHint"
msgstr "După ce ai citit aceste instrucțiuni, apasă 'Următorul' pentru a porni serverul. Apoi urmează pașii pe TV."

#: TV Setup - Series E (2012)
msgid "TvSetup.E.Step1.Title"
msgstr "Deschide Smart Hub"

msgid "TvSetup.E.Step1.Desc"
msgstr "Apasă butonul Smart Hub de pe telecomandă, apoi apasă butonul roșu A pentru a deschide meniul de autentificare."

msgid "TvSetup.E.Step2.Title"
msgstr "Autentifică-te ca dezvoltator"

msgid "TvSetup.E.Step2.Desc"
msgstr "Introdu 'develop' ca nume de cont. TV-ul va genera o parolă - notează-o. Apasă Autentificare."

msgid "TvSetup.E.Step3.Title"
msgstr "Deschide setările dezvoltatorului"

msgid "TvSetup.E.Step3.Desc"
msgstr "Apasă butonul Instrumente de pe telecomandă, mergi la Setări, apoi selectează Dezvoltator."

msgid "TvSetup.E.Step4.Title"
msgstr "Introdu adresa IP"

msgid "TvSetup.E.Step4.Desc"
msgstr "Selectează 'Adresă IP' și introdu: {0}"

msgid "TvSetup.E.Step5.Title"
msgstr "Sincronizează aplicațiile"

msgid "TvSetup.E.Step5.Desc"
msgstr "Selectează 'Sincronizare aplicații utilizator' pentru a începe instalarea. Aplicația va apărea în Smart Hub."

#: TV Setup - Series F (2013)
msgid "TvSetup.F.Step1.Title"
msgstr "Deschide contul Samsung"

msgid "TvSetup.F.Step1.Desc"
msgstr "Apasă butonul Meniu, mergi la Funcții Smart, apoi selectează Cont Samsung."

msgid "TvSetup.F.Step2.Title"
msgstr "Autentifică-te ca dezvoltator"

msgid "TvSetup.F.Step2.Desc"
msgstr "Introdu 'develop' ca email. Lasă parola goală (sau încearcă 'sso1029dev!' dacă este necesar). Apasă Autentificare."

msgid "TvSetup.F.Step3.Title"
msgstr "Deschide setările aplicațiilor"

msgid "TvSetup.F.Step3.Desc"
msgstr "Apasă Smart Hub, mergi la Mai multe aplicații (jos-dreapta), apoi apasă Opțiuni."

msgid "TvSetup.F.Step4.Title"
msgstr "Introdu adresa IP"

msgid "TvSetup.F.Step4.Desc"
msgstr "Selectează 'Setări IP' și introdu: {0}"

msgid "TvSetup.F.Step5.Title"
msgstr "Sincronizează aplicațiile"

msgid "TvSetup.F.Step5.Desc"
msgstr "Selectează 'Pornire sincronizare aplicații' pentru a începe instalarea."

msgid "TvSetup.F.Step6.Title"
msgstr "Repornește dacă este necesar"

msgid "TvSetup.F.Step6.Desc"
msgstr "Dacă aplicațiile nu apar, repornește TV-ul și verifică din nou Smart Hub."

#: TV Setup - Series H (2014-15)
msgid "TvSetup.H.Step1.Title"
msgstr "Deschide contul Samsung"

msgid "TvSetup.H.Step1.Desc"
msgstr "Mergi la Smart Hub, selectează Cont Samsung, apoi selectează Autentificare."

msgid "TvSetup.H.Step2.Title"
msgstr "Autentifică-te ca dezvoltator"

msgid "TvSetup.H.Step2.Desc"
msgstr "Introdu 'develop' ca nume de utilizator. Sari peste câmpul parolă. Bifează 'Ține minte' și apasă Autentificare."

msgid "TvSetup.H.Step3.Title"
msgstr "Deschide meniul de sincronizare"

msgid "TvSetup.H.Step3.Desc"
msgstr "În Smart Hub, îndreaptă telecomanda spre orice aplicație și ține apăsat centrul D-pad-ului până apare un meniu."

msgid "TvSetup.H.Step4.Title"
msgstr "Introdu adresa IP"

msgid "TvSetup.H.Step4.Desc"
msgstr "Selectează 'Setare IP' și introdu: {0} (confirmă fiecare grup de numere cu centrul D-pad-ului)."

msgid "TvSetup.H.Step5.Title"
msgstr "Pornește sincronizarea"

msgid "TvSetup.H.Step5.Desc"
msgstr "Deschide meniul din nou și selectează 'Start User App Sync'."

msgid "TvSetup.H.Step6.Title"
msgstr "Finalizează instalarea"

msgid "TvSetup.H.Step6.Desc"
msgstr "Acceptă promptul de instalare. Ieși din Smart Hub și intră din nou pentru a vedea aplicațiile instalate."

